def condicionales(x,y,z):
    if x>z:
        print('Vamos a hacer una operacion con los valores:', (x**2+y**5+z**4)**6)
    elif x<y:
        print('Vamos a hacer una operacion con los valores:', (x+y+z)*z**3)
    else:
        print('Vamos a hacer una operacion con los valores:', ((z**3)*(x**2))+y)
condicionales(10,2,1)
condicionales(1,2,10)
condicionales(1,1,1)
    
        